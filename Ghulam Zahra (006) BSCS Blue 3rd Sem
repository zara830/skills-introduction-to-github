    {
      "cell_type": "code",
      "source": [
        "#LAB TASK # 20 Input month number\n",
        "month = int(input(\"Enter month number (1-12): \"))\n",
        "\n",
        "# Check number of days\n",
        "if month in [1,3,5,7,8,10,12]:\n",
        "    print(\"31 days\")\n",
        "elif month in [4,6,9,11]:\n",
        "    print(\"30 days\")\n",
        "elif month == 2:\n",
        "    print(\"28 or 29 days\")\n",
        "else:\n",
        "    print(\"Invalid month\")\n"
