    {
      "cell_type": "code",
      "source": [
        "# Lab Task 4: Using Built-in Functions\n",
        "\n",
        "# Taking full name input\n",
        "full_name = input(\"Enter your full name: \")\n",
        "\n",
        "\n",
        "# Taking 5 subject marks and storing in a list\n",
        "marks = []\n",
        "\n",
        "for i in range(5):\n",
        "    mark = float(input(f\"Enter marks for subject {i+1}: \"))\n",
        "    marks.append(mark)\n",
        "\n",
        "# Calculations using built-in functions\n",
        "name_length = len(full_name)\n",
        "marks_type = type(marks)\n",
        "highest_marks = max(marks)\n",
        "lowest_marks = min(marks)\n",
        "total_marks = sum(marks)\n",
        "average_marks = total_marks / len(marks)\n",
        "absolute_difference = abs(highest_marks - lowest_marks)\n",
        "rounded_average = round(average_marks, 2)\n",
        "sorted_marks = sorted(marks)\n",
        "\n",
        "# Displaying results\n",
        "print(\"\\n===== Student Analysis Report =====\")\n",
        "print(\"Length of Name:\", name_length)\n",
        "print(\"Data Type of Marks List:\", marks_type)\n",
        "print(\"Highest Marks:\", highest_marks)\n",
        "print(\"Lowest Marks:\", lowest_marks)\n",
        "print(\"Total Marks:\", total_marks)\n",
        "print(\"Average Marks:\", average_marks)\n",
        "print(\"Absolute Difference (High - Low):\", absolute_difference)\n",
        "print(\"Rounded Average (2 decimal places):\", rounded_average)\n",
        "print(\"Sorted Marks:\", sorted_marks)\n"
      ],
