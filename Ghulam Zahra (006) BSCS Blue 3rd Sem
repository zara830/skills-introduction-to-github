        "def calculator(num1, num2, operator):\n",
        "    #LAB TASK # 30 Performs +, -, *, / operations\n",
        "    if operator == '+':\n",
        "        return num1 + num2\n",
        "    elif operator == '-':\n",
        "        return num1 - num2\n",
        "    elif operator == '*':\n",
        "        return num1 * num2\n",
        "    elif operator == '/':\n",
        "        if num2 != 0:\n",
        "            return num1 / num2\n",
        "        else:\n",
        "            return \"Cannot divide by zero\"\n",
        "    else:\n",
        "        return \"Invalid operator\"\n",
        "\n",
