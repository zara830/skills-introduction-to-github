        "# Task 2: Monthly Budget\n",
        "\n",
        "monthly_income = float(input(\"Enter Monthly Income: \"))\n",
        "house_rent = float(input(\"Enter House Rent: \"))\n",
        "utility_bills = float(input(\"Enter Utility Bills: \"))\n",
        "other_expenses = float(input(\"Enter Other Expenses: \"))\n",
        "\n",
        "total_expenses = house_rent + utility_bills + other_expenses\n",
        "remaining_balance = monthly_income - total_expenses\n",
        "\n",
        "print(\"\\n===== Monthly Budget Summary =====\")\n",
        "print(f\"Total Expenses   : {total_expenses:.2f}\")\n",
        "print(f\"Remaining Balance: {remaining_balance:.2f}\")\n"
      ],
